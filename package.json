{
  "name": "apartments_gallery_holidu",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "0.9.5",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.2",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-junit": "^10.0.0",
    "jest-mock": "^25.1.0",
    "jest-serializer-html": "^7.0.0",
    "react-redux": "^7.1.3",
    "react-test-renderer": "^16.12.0",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
